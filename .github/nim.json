{
  "problemMatcher": [
    {
      "owner": "nim",
      "pattern": [
        {
          "regexp": "^(.+)\\((\\d+),\\s(\\d+)\\)\\s(Error|Warning|Hint):\\s(.+)\\s\\[.+\\]$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5,
          "code": 6
        }
      ]
    }
  ]
}
